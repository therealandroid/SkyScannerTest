<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PrimaryTextBlackStyle">
        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textColor">@color/textPrimaryColor</item>
    </style>

    <style name="SecondaryTextBlackStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textColor">@color/textSecondaryColor</item>
    </style>

    <style name="AlertViewTitleStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textColor">@color/alertViewPrimaryColor</item>
    </style>

    <style name="ToolbarTitleStyle">
        <item name="android:textSize">22sp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textColor">@color/toolbarTitleColor</item>
    </style>

    <style name="ToolbarSubtitleStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textColor">@color/toolbarSubtitleColor</item>
    </style>
</resources>