<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
            android:id="@+id/I1_outcomeContent"
                  android:orientation="horizontal"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:layout_marginStart="16dp"
                  android:layout_marginEnd="16dp">

        <ImageView
                android:id="@+id/I1_outcomeCarrierImage"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_weight="0"/>

        <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_weight="1">

            <TextView
                    android:id="@+id/I1_outcomeDepartureAndArrivalTime"
                    android:layout_width="wrap_content"
                    style="@style/PrimaryTextBlack"
                    android:text="sdfdsfsdfsd"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:id="@+id/I1_outcomeCarrierDisplayCodeAndName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/SecondaryTextBlack"
                    android:text="dsfdfsdf"
                    android:layout_below="@+id/I1_outcomeDepartureAndArrivalTime"/>

        </RelativeLayout>

        <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp">

            <TextView
                    android:id="@+id/I1_outcomeDirectionType"
                    android:layout_width="wrap_content"
                    style="@style/PrimaryTextBlack"
                    android:text="sdfdsfsdfsd"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:id="@+id/I1_outcomeTotalFlightDuration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/SecondaryTextBlack"
                    android:text="dsfdfsdf"
                    android:layout_below="@+id/I1_outcomeDirectionType"/>

        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
            android:id="@+id/I1_incomeContent"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_below="@+id/I1_outcomeContent"
            android:layout_marginEnd="16dp">

        <ImageView
                android:id="@+id/I1_incomeCarrierImage"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_weight="0"/>

        <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_weight="1">

            <TextView
                    android:id="@+id/I1_incomeDepartureAndArrivalTime"
                    android:layout_width="wrap_content"
                    style="@style/PrimaryTextBlack"
                    android:text="sdfdsfsdfsd"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:id="@+id/I1_incomeCarrierDisplayCodeAndName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/SecondaryTextBlack"
                    android:text="dsfdfsdf"
                    android:layout_below="@+id/I1_incomeDepartureAndArrivalTime"/>

        </RelativeLayout>

        <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp">

            <TextView
                    android:id="@+id/I1_incomeDirectionType"
                    android:layout_width="wrap_content"
                    style="@style/PrimaryTextBlack"
                    android:text="sdfdsfsdfsd"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:id="@+id/I1_incomeTotalFlightDuration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/SecondaryTextBlack"
                    android:text="dsfdfsdf"
                    android:layout_below="@+id/I1_incomeDirectionType"/>

        </RelativeLayout>
    </LinearLayout>


</RelativeLayout>